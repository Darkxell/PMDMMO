<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.darkxell.dataeditor.application.controller.cutscene.SelectEventTypeController">
	<children>
		<ListView fx:id="eventList" prefHeight="200.0" prefWidth="200.0" VBox.Vgrow="ALWAYS" />
		<Button mnemonicParsing="false" onAction="#onCancel" text="Cancel">
			<VBox.margin>
				<Insets bottom="10.0" top="10.0" />
			</VBox.margin>
		</Button>
	</children>
</VBox>
